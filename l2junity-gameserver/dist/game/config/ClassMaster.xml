				<item id="5575" count="100000" /> <!-- 100k Ancient Adena for second class -->
			</conditions>
		</classChangeOption>
		<classChangeOption name="Pay Adena and Ancient Adena">
			<appliesTo>
				<category>THIRD_CLASS_GROUP</category>
			</appliesTo>
			<conditions>
				<item id="57" count="10000000" /> <!-- 10kk Adena for third class -->
				<item id="5575" count="1000000" /> <!-- 1kk Ancient Adena for third class -->
			</conditions>
		</classChangeOption>
		<classChangeOption name="Pay more Adena for a chance to not pay Ancient Adena">
			<appliesTo>
				<category>THIRD_CLASS_GROUP</category>
			</appliesTo>
			<conditions>
				<item id="57" count="30000000" /> <!-- 50% chance to take your 30kk Adena for third class -->
				<item id="5575" count="1000000" chance="50" /> <!-- 50% chance to take your 1kk Ancient Adena for third class -->
			</conditions>
		</classChangeOption>
		<classChangeOption name="Pay Adena and Ancient Adena with Noblesse reward">
			<appliesTo>
				<category>THIRD_CLASS_GROUP</category>
			</appliesTo>
			<rewards>
				<setNoble/> <!-- Set noble when this option is used -->
			</rewards>
			<conditions>
				<item id="57" count="100000000" /> <!-- 100kk Adena for third class -->
				<item id="5575" count="10000000" /> <!-- 10kk Ancient Adena for third class -->
			</conditions>
		</classChangeOption>
		<classChangeOption name="Option 1">
			<appliesTo>
				<category>FOURTH_CLASS_GROUP</category>
			</appliesTo>
			<rewards>
				<item id="57" count="100000000" chance="10" /> <!-- 100kk Adena -->
				<setHero/> <!-- Set hero when this option is used -->
			</rewards>
			<conditions>
				<item id="57" count="100000000" /> <!-- 100kk Adena for fourth class -->
				<item id="5575" count="10000000" /> <!-- 10kk Ancient Adena for fourth class -->
			</conditions>
		</classChangeOption>
	</classMaster>
