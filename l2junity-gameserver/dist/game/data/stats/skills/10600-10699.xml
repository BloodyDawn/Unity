	<skill id="10609" levels="1" name="Critical Chances">
		<!-- For 5 min., Critical Damage + 304 and vital spot attack success rate + 15%. Front Critical Damage - 30%, Side Critical Rate + 30% and Critical Damage + 30%. Rear Critical Rate + 50% and Critical Damage + 50%. Requires a dagger or dual dagger. -->
		<set name="abnormalLvl" val="1" />
		<set name="abnormalTime" val="300" />
		<set name="abnormalType" val="FOCUS_DAGGER" />
		<set name="icon" val="icon.skill10878" />
		<set name="operateType" val="A2" />
		<set name="targetType" val="SELF" />
		<set name="magicLvl" val="85" />
		<set name="mpConsume" val="36" />
		<set name="hitTime" val="2000" />
		<set name="effectPoint" val="669" />
		<set name="reuseDelay" val="30000" />
		<cond msgId="113" addName="1">
			<using kind="DAGGER,DUALDAGGER" />
		</cond>
		<for>
			<effect name="Buff">
				<add stat="cAtkAdd" val="304">
					<using kind="DAGGER,DUALDAGGER" />
				</add>
				<mul stat="blowRate" val="1.15">
					<using kind="DAGGER,DUALDAGGER" />
				</mul>
				<mul stat="cAtk" val="0.7"> <!-- Decreases frontal critical attack rate by 30% -->
					<player front="true" />
				</mul>
				<mul stat="rCrit" val="0.3"> <!-- Increase side critical attack rate by 30% -->
					<and>
						<player front="false" />
						<player behind="false" />
					</and>
				</mul>
				<mul stat="cAtk" val="1.3"> <!-- Increase side critical attack damage by 30% -->
					<and>
						<player front="false" />
						<player behind="false" />
					</and>
				</mul>
				<mul stat="rCrit" val="0.5"> <!-- Increase rear critical attack damage by 50% -->
					<player behind="true" />
				</mul>
				<mul stat="cAtk" val="1.5"> <!-- Increase rear critical attack rate by 50% -->
					<player behind="true" />
				</mul>
			</effect>
		</for>
	</skill>
