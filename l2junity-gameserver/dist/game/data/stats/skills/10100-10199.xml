		<set name="coolTime" val="500" />
		<set name="effectPoint" val="2" />
		<set name="reuseDelay" val="600000" />
		<for>
			<effect name="Buff">
				<mul stat="pDef" val="#def" />
				<mul stat="mDef" val="#def" />
				<add stat="waterRes" val="#elemRes" />
				<add stat="fireRes" val="#elemRes" />
				<add stat="windRes" val="#elemRes" />
				<add stat="earthRes" val="#elemRes" />
				<add stat="holyRes" val="#elemRes" />
