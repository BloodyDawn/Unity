		<operateType>A2</operateType>
		<targetType>SELF</targetType>
		<magicLvl>#magicLvl</magicLvl>
		<mpConsume>#mpConsumes</mpConsume>
		<hitTime>3000</hitTime>
		<coolTime>500</coolTime>
		<effectPoint>2</effectPoint>
		<reuseDelay>600000</reuseDelay>
		<for>
			<effect name="Buff">
				<mul stat="pDef" val="#def" />
				<mul stat="mDef" val="#def" />
				<add stat="waterRes" val="#elemRes" />
				<add stat="fireRes" val="#elemRes" />
				<add stat="windRes" val="#elemRes" />
				<add stat="earthRes" val="#elemRes" />
				<add stat="holyRes" val="#elemRes" />
				<add stat="darkRes" val="#elemRes" />
				<add stat="moveSpeed" val="#runSpd" />
				<add stat="accCombat" val="#accCombat">
					<using kind="SWORD,BLUNT" />
				</add>
				<mul stat="pAtkSpd" val="#pAtkSpd">
					<using kind="SWORD,BLUNT" />
				</mul>
				<add stat="rCrit" val="#rCrit">
					<using kind="SWORD,BLUNT" />
				</add>
				<mul stat="cAtk" val="#cAtk">
					<using kind="SWORD,BLUNT" />
				</mul>
			</effect>
		</for>
	</skill>
